                   em:iconURL=""
                   em:optionsURL="chrome://tabextensions/content/pref/prefDialogCall.xul">


    <em:updateURL>https://pub.cozmixng.org/~piro/xul/xul.rdf.en</em:updateURL>


    <em:targetApplication>
      <!-- Firefox -->
      <RDF:Description em:id="{ec8030f7-c20a-464f-9b0e-13a3a9e97384}"
                       em:minVersion="0.8"
                       em:maxVersion="1.5.0.*" />
      <!-- Mozilla Suite -->
      <RDF:Description em:id="{86c18b42-e466-45a9-ae7a-9b95ba6f5640}"
                       em:minVersion="1.0"
                       em:maxVersion="1.8.*" />
    </em:targetApplication>
	<em:file>
		<RDF:Description RDF:about="urn:mozilla:extension:file:tabextensions_focusprevselected.jar">
			<em:package>content/tabextensions_focusprevselected/</em:package>
		</RDF:Description>
	</em:file>
